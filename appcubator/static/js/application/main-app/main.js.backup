            resized: function(e, ui) {
                var deltaHeight = Math.round((ui.size.height + 6) / this.positionVerticalGrid);
                var elem = util.get('widget-wrapper-' + this.model.cid);
                elem.style.width = '';
                elem.style.height = '';
