],
function() {

  var TutorialView = Backbone.View.extend({
    tagName: 'div',
    className: 'tutorial-view hide',
    css : "tutorial",
    expanded: false,

    events : {
      "click #tutorial-menu-list li" : "clickedMenuItem",
      'scroll #tutorial-menu-list' : 'menuScrolled',
