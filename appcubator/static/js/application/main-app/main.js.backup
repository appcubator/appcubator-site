        str += " all " + self.model.entity.get('name')+"s";
      }
      else if(this.model.get('numberOfRows') == 1) {
        str += " a " + self.model.entity.get('name');
      }
      else {
        str += " "+ String(self.model.get('numberOfRows')).replace('-',' ').toLowerCase() + " " + self.model.entity.get('name')+"s";
      }

