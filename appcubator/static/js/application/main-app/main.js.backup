            },

            initialize: function(widgetModel, location) {
                this.model = widgetModel;
                var rowModel = this.model.get('data').get('container_info').get('row');
                RowGalleryView.__super__.initialize.call(this, rowModel.get('uielements'));
                _.bindAll(this);
